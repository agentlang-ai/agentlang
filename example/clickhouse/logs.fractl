(component :Logs)

(entity
 :LogEntry
 {:Id {:type :Int :guid true}
  :Message :String
  :Created :Now
  :Level {:oneof ["info" "debug" "warn" "error"]}})

(require '[fractl.resolver.registry :as r])
(use '[fractl.resolver.click-house])

(r/register-resolver {:name :click-house-logs
                      :type :click-house
                      :paths [:Logs/LogEntry]})
