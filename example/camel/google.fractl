(component :Google)

(require '[fractl.resolver.registry :as r])
(use '[fractl.example.camel.google-sheets-resolver])

(entity
 :Sheet
 {:Properties {:type :Edn :optional true}
  :Id {:type :String :guid true :default "sheet-01"}
  :Url {:type :String :optional true}})

(entity
 :Data
 {:SheetId :String
  :Values :Edn
  :Range :String})

(r/register-resolver {:type :camel-google-sheets :name :cgs01 :paths [:Google/Sheet :Google/Data]})
