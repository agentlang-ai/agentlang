{
  "name": "agentlang",
  "description": "Please enter a brief description here",
  "version": "0.0.1",
  "files": [
    "bin",
    "out",
    "src"
  ],
  "type": "module",
  "scripts": {
    "build": "tsc -b tsconfig.src.json && node esbuild.mjs",
    "build:deno": "deno run -A scripts/build.ts",
    "build:ts": "tsc -b tsconfig.src.json",
    "build:vite": "vite build --emptyOutDir",
    "watch": "concurrently -n tsc,esbuild -c blue,yellow \"tsc -b tsconfig.src.json --watch\" \"node esbuild.mjs --watch\"",
    "watch:deno": "deno run --watch -A scripts/build.ts",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "lint:deno": "deno lint scripts/",
    "format": "prettier --write \"src/**/*.{js,ts,tsx,json,md}\"",
    "format:deno": "deno fmt scripts/",
    "format:check": "prettier --check \"src/**/*.{js,ts,tsx,json,md}\"",
    "langium:generate": "langium generate",
    "langium:generate:production": "langium generate --mode=production",
    "langium:watch": "langium generate --watch",
    "vscode:prepublish": "npm run build && npm run lint",
    "build:web": "npm run build",
    "bundle": "vite build",
    "bundle:serve": "http-server ./dist --port 5175",
    "dev": "vite",
    "dev:debug": "vite --debug --force",
    "serve": "npm run dev",
    "repl": "deno task repl",
    "run": "deno task run",
    "test": "vitest run",
    "test:deno": "deno test -A scripts/"
  },
  "dependencies": {
    "@aws-sdk/client-cognito-identity": "^3.828.0",
    "@aws-sdk/client-cognito-identity-provider": "^3.828.0",
    "@aws-sdk/credential-providers": "^3.828.0",
    "@codingame/monaco-vscode-editor-service-override": "^17.1.0",
    "@codingame/monaco-vscode-keybindings-service-override": "^17.1.0",
    "@isomorphic-git/lightning-fs": "^4.6.1",
    "@langchain/core": "^0.3.59",
    "@langchain/openai": "^0.5.13",
    "@types/express": "^5.0.1",
    "amazon-cognito-identity-js": "^6.3.15",
    "aws-jwt-verify": "^5.1.0",
    "buffer": "^6.0.3",
    "c12": "^3.0.4",
    "chalk": "~5.3.0",
    "commander": "~11.0.0",
    "dotenv": "^16.5.0",
    "express": "^5.1.0",
    "fs-extra": "^11.3.0",
    "generator-langium": "^3.0.0",
    "jsonwebtoken": "^9.0.2",
    "jwk-to-pem": "^2.0.7",
    "langchain": "^0.3.28",
    "langium": "^3.5.0",
    "monaco-editor": "~0.52.0",
    "monaco-editor-wrapper": "^6.7.0",
    "node-fetch": "^2.7.0",
    "pg": "^8.16.1",
    "pgvector": "^0.2.1",
    "sqlite3": "^5.1.7",
    "typeorm": "^0.3.23",
    "vscode-languageclient": "^9.0.1",
    "vscode-languageserver": "^9.0.1",
    "winston": "^3.17.0",
    "winston-daily-rotate-file": "^5.0.0",
    "zod": "^3.25.55"
  },
  "overrides": {
    "semver": "^7.5.3",
    "trim-newlines": ">=3.0.1"
  },
  "devDependencies": {
    "@codingame/esbuild-import-meta-url-plugin": "~1.0.2",
    "@eslint/js": "^9.26.0",
    "@types/cookie-parser": "^1.4.9",
    "@types/node": "^18.19.110",
    "@types/vscode": "^1.100.0",
    "@typescript-eslint/eslint-plugin": "~8.32.1",
    "@typescript-eslint/parser": "~8.32.1",
    "brace-expansion": ">=2.0.2",
    "concurrently": "~8.2.1",
    "esbuild": "^0.25.4",
    "eslint": "~9.26.0",
    "http-server": "~14.1.1",
    "langium-cli": "^3.5.0",
    "monaco-languageclient": "^9.7.0",
    "nodemon": "^3.1.10",
    "prettier": "^3.5.3",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.32.1",
    "vite": "^6.3.5",
    "vite-plugin-node-polyfills": "^0.23.0",
    "vitest": "^3.1.3",
    "vscode-languageclient": "^9.0.1",
    "vscode-uri": "^3.1.0"
  },
  "volta": {
    "node": ">=20.0.0",
    "npm": ">=10.8.2"
  },
  "displayName": "agentlang",
  "engines": {
    "vscode": "^1.67.0",
    "node": ">=20.0.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "agentlang",
        "aliases": [
          "Agentlang",
          "agentlang"
        ],
        "extensions": [
          ".al"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "agentlang",
        "scopeName": "source.agentlang",
        "path": "syntaxes/agentlang.tmLanguage.json"
      }
    ]
  },
  "activationEvents": [
    "onLanguage:agentlang"
  ],
  "main": "./out/extension/main.cjs",
  "bin": {
    "agentlang-cli": "./bin/cli.js"
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "eslint --fix --cache",
      "prettier --write"
    ],
    "*.{json,md,yml,yaml}": [
      "prettier --write"
    ]
  }
}
